pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo "Code is already cloned by Jenkinsfile job"
            }
        }

        stage('Build') {
            steps {
                echo "Building the project..."
            }
        }

        stage('Test') {
            steps {
                echo "Testing the project..."
            }
        }
    }
}

